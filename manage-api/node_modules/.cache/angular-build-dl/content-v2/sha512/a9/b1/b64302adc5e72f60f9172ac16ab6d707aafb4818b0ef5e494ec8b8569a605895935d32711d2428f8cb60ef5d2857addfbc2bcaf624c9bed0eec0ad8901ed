class ViewQuestionsComponent {
    constructor(helper, route, peralService, _location) {
        this.helper = helper;
        this.route = route;
        this.peralService = peralService;
        this._location = _location;
        this.totalLength = [10, 25, 50, 100];
        this.displayedColumns = [
            'title',
        ];
    }
    getPeralByUuid() {
        return this.route.paramMap.pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_7__["switchMap"])((params) => {
            return this.peralService.viewQuestions(params.get('uuid'));
        }));
    }
    ngOnInit() {
        this.getPeralByUuid().subscribe(data => {
            this.perals$ = data.response;
            this.filteredQuestions = new _angular_material_table__WEBPACK_IMPORTED_MODULE_4__["MatTableDataSource"](this.perals$.queIds);
            this.filteredQuestions.paginator = this.paginator;
        });
    }
    filterList(event) {
        const filterValue = event.target.value.trim();
        this.filteredQuestions.filter = filterValue.trim().toLowerCase();
        if (this.filteredQuestions.paginator) {
            this.filteredQuestions.paginator.firstPage();
        }
    }
    backToPerals() {
        this._location.back();
    }
}
