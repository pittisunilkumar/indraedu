  "scripts": {
    "serve": "node dist/apps/test-bank-api/main.js",
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "start:usersfe": "ng serve users-client --port 4200 -o",
    "start:usersbe": "ng serve student-api",
    "start:managefe": "ng serve manage-client --port 4201 -o --host 0.0.0.0",
    "start:managebe": "ng serve manage-api ",
    "manage-dev": "concurrently -p=\"{name}\" -n=\"Angular,NestJS\" -c=\"magenta,cyan\" \"npm run start:managebe\" \"npm run start:managefe\"",
    "manage-build": "nx build manage-api --prod && nx build manage-client --prod --base-href https://admin.indraedu.com/ && npm pack && npm run copy:manage-htaccess ",
    "student-build": "nx build student-api --prod && nx build users-client --prod --base-href / && npm run copy:students-htaccess && npm pack",
    "copy:manage-htaccess": "cp .htaccess_manage dist/apps/manage-client/.htaccess",
    "copy:students-htaccess": "cat jenkins_user | sudo -S cp -f .htaccess_students dist/apps/users-client/.htaccess",
    "mac:manage-build": "nx build manage-api --prod && nx build manage-client --prod --base-href / && npm run copy-mac:manage-htaccess && npm pack",
    "mac:student-build": "nx build student-api --prod && nx build users-client --prod --base-href / && npm run copy-mac:students-htaccess && npm pack",
    "copy-mac:manage-htaccess": "cp .htaccess_manage dist/apps/manage-client/.htaccess",
    "copy-mac:students-htaccess": "cp .htaccess_students dist/apps/users-client/.htaccess",
    "deploy-manage": "pm2 delete janu && pm2 start dist/apps/manage-api/main.js --name=janu && pm2 save",
    "deploy-users": "pm2 delete students-api && pm2 start dist/apps/student-api/main.js --name=students-api && pm2 save",
    "dev:all": "npm run manage-dev",
    "fix:bcrypt": "npm install node-gyp -g && npm install bcrypt -g && npm i bcrypt",
    "pre-deploy": "npm i --production --ignore-scripts && npm run fix:bcrypt",
    "deploy:users": "npm run pre-deploy && npm run deploy-users",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": true,
